<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/registro"
    tools:context=".registroSolicitante" >

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.073" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="328dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.979">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#FFFEFE"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/crea_cuenta" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:layout_width="329dp"
        android:layout_height="712dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.649" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="1000dp"
                android:text="TextView"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/editTextText3"
                android:layout_width="298dp"
                android:layout_height="40dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/txtregistro"
                android:ems="10"
                android:hint="Nombre"
                android:inputType="text"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/editTextText4"
                android:layout_width="298dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/txtregistro"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                app:layout_constraintTop_toBottomOf="@+id/editTextText3"
                tools:layout_editor_absoluteX="16dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"/>

            <EditText
                android:id="@+id/editTextText8"
                android:layout_width="298dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/txtregistro"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                app:layout_constraintTop_toBottomOf="@+id/editTextText4"
                tools:layout_editor_absoluteX="15dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"/>

            <EditText
                android:id="@+id/editTextText9"
                android:layout_width="298dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/txtregistro"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                app:layout_constraintTop_toBottomOf="@+id/editTextText8"
                tools:layout_editor_absoluteX="15dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"/>

            <EditText
                android:id="@+id/editTextText10"
                android:layout_width="298dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/txtregistro"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                app:layout_constraintTop_toBottomOf="@+id/editTextText9"
                tools:layout_editor_absoluteX="15dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"/>

            <EditText
                android:id="@+id/editTextText11"
                android:layout_width="298dp"
                android:layout_height="40dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/txtregistro"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                app:layout_constraintTop_toBottomOf="@+id/editTextText10"
                tools:layout_editor_absoluteX="15dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"/>

            <Spinner
                android:id="@+id/spinner"
                android:layout_width="228dp"
                android:layout_height="29dp"
                android:layout_marginTop="2dp"
                app:layout_constraintTop_toBottomOf="@+id/textView17"
                tools:layout_editor_absoluteX="16dp"
                />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Genero"
                android:textSize="13sp"
                app:layout_constraintTop_toBottomOf="@+id/editTextText11"
                tools:layout_editor_absoluteX="15dp" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Area de trabajo de preferencia"
                android:textSize="13dp"
                app:layout_constraintTop_toBottomOf="@+id/spinner"
                tools:layout_editor_absoluteX="16dp" />

            <Spinner
                android:id="@+id/spinner2"
                android:layout_width="228dp"
                android:layout_height="29dp"
                android:layout_marginTop="2dp"
                app:layout_constraintTop_toBottomOf="@+id/textView18"
                tools:layout_editor_absoluteX="16dp" />

            <EditText
                android:id="@+id/editTextTextMultiLine"
                android:layout_width="290dp"
                android:layout_height="65dp"
                android:ems="10"
                android:gravity="start|top"
                android:inputType="textMultiLine"
                tools:layout_editor_absoluteX="21dp"
                tools:layout_editor_absoluteY="514dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>